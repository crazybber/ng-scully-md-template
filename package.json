{
  "name": "pipiblog",
  "version": "0.0.1",
  "description": "angular scully web template",
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "build": "ng build",
    "prettier": "prettier --write \"**/*.{js,json,css,scss,less,md,ts,html,component.html}\"",
    "test": "ng test",
    "lint": "ng lint",
    "e2e": "ng e2e",
    "scully": "scully",
    "scully:serve": "scully serve",
    "sr": "npm run scully -- --scanRoutes",
    "sw": "scully --watch",
    "ss": "scully serve",
    "post": "ng g @scullyio/init:post --metaDataFile='blog-meta.yaml' --target='blog'",
    "commit": "git add . && npx git-cz",
    "commit:select": "npx git-cz",
    "deploy:gh-pages": "git subtree push --prefix dist/site-blog origin gh-pages"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "~9.1.6",
    "@angular/common": "~9.1.6",
    "@angular/compiler": "~9.1.6",
    "@angular/core": "~9.1.6",
    "@angular/forms": "~9.1.6",
    "@angular/platform-browser": "~9.1.6",
    "@angular/platform-browser-dynamic": "~9.1.6",
    "@angular/router": "~9.1.6",
    "@scullyio/init": "0.0.26",
    "@scullyio/ng-lib": "latest",
    "@scullyio/scully": "latest",
    "@types/jquery": "^3.3.38",
    "@types/materialize-css": "^1.0.7",
    "jquery": "^3.5.1",
    "marked": "^1.0.0",
    "materialize-css": "^0.100.2",
    "ngx-markdown": "^9.0.0",
    "prismjs": "^1.20.0",
    "rxjs": "~6.5.5",
    "tslib": "^1.11.2",
    "zone.js": "~0.10.3"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "~0.901.5",
    "@angular/cli": "~9.1.6",
    "@angular/compiler-cli": "~9.1.6",
    "@angular/language-service": "~9.1.6",
    "@types/jasmine": "~3.5.10",
    "@types/jasminewd2": "~2.0.8",
    "@types/node": "^13.13.5",
    "codelyzer": "^5.2.2",
    "conventional-changelog": "^3.1.21",
    "cz-conventional-changelog": "^3.2.0",
    "husky": "4.2.5",
    "jasmine-core": "~3.5.0",
    "jasmine-spec-reporter": "~5.0.2",
    "karma": "~5.0.5",
    "karma-chrome-launcher": "~3.1.0",
    "karma-coverage-istanbul-reporter": "~3.0.2",
    "karma-jasmine": "~3.1.1",
    "karma-jasmine-html-reporter": "^1.5.3",
    "lint-staged": "10.2.2",
    "npm-check-updates": "^5.0.0",
    "prettier": "2.0.5",
    "protractor": "~5.4.4",
    "ts-node": "~8.10.1",
    "tslint": "~6.1.2",
    "typescript": "~3.8.3"
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "*.{js,json,css,scss,less,md,ts,html,component.html}": [
      "prettier --write",
      "git add"
    ]
  }
}
